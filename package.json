{
  "name": "b2b-marketplace-frontend",
  "version": "0.1.0",
  "private": true,
  "author": "B2B Frontend Team",
  "homepage": ".",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "dotenv": "^16.3.1",
    "gh-pages": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.14.2",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.4",
    "react-use-refs": "^1.0.1",
    "redux-persist": "^6.0.0",
    "sass": "^1.64.2",
    "sass-loader": "^13.3.2",
    "swiper": "^10.2.0",
    "validator": "^13.11.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build --profile",
    "eject": "react-scripts eject",
    "format": "prettier . --write",
    "lint": "npx eslint . --ext .js,jsx",
    "lint-fix": "npm run lint -- --fix --color",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prepaer": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.1",
    "prop-types": "^15.8.1",
    "webpack": "^5.88.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "npm run lint-fix",
    "*.{js,jsx,css,md,html,json}": "npm run format"
  }
}
