{
  "name": "B2Buy Marketplace",
  "version": "0.1.0",
  "private": true,
  "author": "B2Buy Frontend Team",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.14.2",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.4",
    "react-use-refs": "^1.0.1",
    "redux-persist": "^6.0.0",
    "swiper": "^10.2.0",
    "validator": "^13.11.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production react-scripts start",
    "dev": "cross-env NODE_ENV=development react-scripts start",
    "build": "cross-env NODE_ENV=production react-scripts build --profile",
    "eject": "react-scripts eject",
    "format": "prettier . --write",
    "lint": "npx eslint . --ext .js,jsx",
    "lint-fix": "npx eslint --ext .js,jsx --fix --color .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prepaer": "husky install",
    "sb": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@storybook/addon-essentials": "^7.5.0",
    "@storybook/addon-interactions": "^7.5.0",
    "@storybook/addon-links": "^7.5.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.0",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^7.5.0",
    "@storybook/react-webpack5": "^7.5.0",
    "@storybook/testing-library": "^0.2.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-storybook": "^0.6.15",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.1",
    "prop-types": "^15.8.1",
    "sass": "^1.64.2",
    "sass-loader": "^13.3.2",
    "storybook": "^7.5.0",
    "webpack": "^5.88.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint-fix",
      "npm run format"
    ],
    "*.{scss,css,md,html,json}": "npm run format"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
